<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="teamvelociraptor.assignment4">

    <uses-sdk tools:overrideLibrary="com.firebase.ui, com.firebase.ui.auth" />

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Velocibux"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MessagingActivity" android:theme="@style/MessagingTheme" />
        <activity
            android:name=".FriendsListActivity"
            android:label="@string/title_activity_friends_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".PaymentActivity" />
        <activity android:name=".TransactionHistoryActivity" />
        <activity
            android:name=".QRActivity"
            android:label="@string/title_activity_qr"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".QRCameraActivity"
            android:label="@string/title_activity_qrcamera"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".TempActivity"></activity>
             />
        <activity android:name=".AccountBalance" android:theme="@style/AccountTheme"></activity>

    </application>

</manifest>